   "source": [
    "### Let's create a list of team db's that we'd like to check for fresh scores:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "dc0486db",
   "metadata": {},
   "outputs": [],
   "source": [
    "SCHEMA = \"dw\"\n",
    "TABLE = \"customerretentionscores\"\n",
    "TEAMS = json.load(open('../../teamsconfig.json'))\n",
    "USER = \"admin\""
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e6263e6f",
   "metadata": {},
   "source": [
    "### Now we can loop through each datatbase, connect to it and check the retention scores:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "1fde9af7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "CHECKING RETENTION SCORES\n",
      " > NO RETENTION SCORES FOUND FOR: Portland Sea Dogs - milbseadogs (100)\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "(37, 6)"
      ]
     },
     "execution_count": 12,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "print(f\"CHECKING RETENTION SCORES\")\n",
    "\n",
    "results = []\n",
    "for team in TEAMS:\n",
    "\n",
    "    conn = wr.data_api.redshift.connect(\n",
    "        cluster_id = team['cluster'],\n",
    "        database =  team['dbname'].lower(),\n",
    "        db_user = USER\n",
    "    )\n",
    "\n",
    "    sql = f\"\"\"\n",
    "        SELECT count(*), lkupclientid, MAX(scoredate) as max_score_date\n",
    "        FROM {team['dbname'].lower()}.{SCHEMA}.{TABLE} \n",
    "        WHERE lkupclientid = {team['lkupclientid']}\n",
    "        GROUP BY lkupclientid\n",
    "    \"\"\"\n",
    "\n",
    "    df = wr.data_api.redshift.read_sql_query(\n",
    "        sql = sql, \n",
    "        con = conn\n",
    "    )\n",
    "\n",
    "\n",
    "    df[\"cluster\"] = team[\"cluster\"]\n",
    "    df[\"dbname\"] = team[\"dbname\"]\n",
    "    df[\"lkupclientid\"] = team[\"lkupclientid\"]\n",
    "    df[\"name\"] = team[\"name\"]\n",
    "\n",
    "    if len(df) > 0:\n",
    "        results.append(df)\n",
    "    else:\n",
    "        print(f\" > NO RETENTION SCORES FOUND FOR: {team['name']} - {team['clientcode']} ({team['lkupclientid']})\")\n",
    "\n",
    "df_results = pd.concat(results)\n",
    "\n",
    "df_results[\"count\"] = pd.to_numeric(df_results[\"count\"])\n",
    "df_results[\"max_score_date\"] = pd.to_datetime(df_results[\"max_score_date\"])\n",
    "\n",
    "df_results.shape\n"
   ]
